generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model registros {
  idRegister                   Int       @id @default(autoincrement())
  id_user                      Int
  createdAt                    DateTime? @default(now()) @db.DateTime(0)
  talon_name                   String?   @db.VarChar(255)
  calcomania_verificacion_name String?   @db.VarChar(255)
  tarjeta_circulacion_name     String?   @db.VarChar(255)
  placa_auto_name              String?   @db.VarChar(255)
  placa_engomado_name          String?   @db.VarChar(255)
  vin_parabrisas_name          String?   @db.VarChar(255)
  vin_carroceria_name          String?   @db.VarChar(255)
  no_motor_name                String?   @db.VarChar(255)
  calcomania_repuve_name       String?   @db.VarChar(255)
  tacometro_name               String?   @db.VarChar(255)
  vin_escanner_name            String?   @db.VarChar(255)
  km_escanner_name             String?   @db.VarChar(255)
}

